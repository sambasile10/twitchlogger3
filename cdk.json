{
    "app": "npx ts-node -P tsconfig.json --prefer-ts-exts src/Stack.ts",
    "output": "cdk.out",
    "profile": "personal",
    "watch": {
        "include": [
            "src/**/*.ts"
        ],
        "exclude": [
            "README.md",
            "cdk*.json",
            "**/*.d.ts",
            "**/*.js",
            "tsconfig.json",
            "package*.json",
            "yarn.lock",
            "node_modules"
        ]
    }
}